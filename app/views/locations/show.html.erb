<table border="2px">
	<thead>
		<tr>
			<th>Heading:</th>
			<th>Room:</th>
			<th>Bedrooms:</th>
			<th>Beds:</th>
			<th>Bathrooms</th>
			<th>Location</th>
			<th>Ocuupancy</th>
			<th>Description</th>
			<th>Price Monthly</th>
			<th>Price Nightly</th>
			<th>Price Weekend</th>
			<th>Price Weekly</th>
			<th>Photo</th>
			<th>Address</th>
			<th>Provider</th>
			<th>Url</th>
			<th>Rating</th>
			</tr
		</tr>
	</thead>
	<tbody>
		
<%@results.each do |result|%>
	<tr>
	<td><%= result["attr"]["heading"]%></td>
	<td><%= result["attr"]["roomType"]["text"]%></td>
	<td><%= result["attr"]["bedrooms"]%></td>
	<td><%= result["attr"]["beds"]%></td>
	<td><%if result["attr"]["bathrooms"] < 0%>
			<p>None</p>
			<%else%>
				<%=result["attr"]["bathrooms"]%>
			<%end%></td>
	<td><%= result["latLng"]%></td>
	<td><%= result["attr"]["occupancy"]%></td>
	<td><%= result["attr"]["description"]%></td>
	<td><%= result["price"]["monthly"]%></td>
	<td><%= result["price"]["nightly"]%></td>
	<td><%= result["price"]["weekend"]%></td>
	<td><%= result["price"]["weekly"]%></td>
	<td><%= image_tag result["photos"][0]["large"]%></td>
	<td><%= result["location"]["all"]%></td>
	<td><%= result["provider"]["full"]%></td>
	<td><%= result["provider"]["url"]%></td>
	<td><%if result["reviews"]["rating"].to_i <0 %>
					<p>None</p>
			<%else%>
				<%=result["reviews"]["rating"]%>
			<%end%></td>

		</tr>
	

<%end%>
</tbody>
</table>